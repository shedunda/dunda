exec sp_executesql N' SET FMTONLY OFF; SET NO_BROWSETABLE ON;SELECT T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID, T_ORG_ACCOUNTSYSTEM.FDOCUMENTSTATUS, T_ORG_ACCOUNTSYSTEM.FFORBIDSTATUS, T_ORG_ACCOUNTSYSTEM.FNUMBER, T_ORG_ACCOUNTSYSTEM.FMODIFIERID, T_ORG_ACCOUNTSYSTEM.FCREATORID, T_ORG_ACCOUNTSYSTEM.FCREATEDATE, T_ORG_ACCOUNTSYSTEM.FMODIFYDATE, T_ORG_ACCOUNTSYSTEM.FFORBIDDATE, T_ORG_ACCOUNTSYSTEM.FAUDITDATE, T_ORG_ACCOUNTSYSTEM.FFORBIDORID, T_ORG_ACCOUNTSYSTEM.FAUDITORID, T_ORG_ACCOUNTSYSTEM.FISDEFAULT, T_ORG_ACCOUNTSYSTEM.FVERSIONSNO, T_ORG_ACCOUNTSYSTEM.FNEWVERSIONSID, T_ORG_ACCOUNTSYSTEM.FISORIGINAL, T_ORG_ACCOUNTSYSTEM.FVERSIONSSEQ, T_ORG_ACCOUNTSYSTEM.FISCORPORATE FROM T_ORG_ACCOUNTSYSTEM WHERE T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID = @PKValue;
SELECT T_ORG_ACCOUNTSYSTEM_L.FPkId, T_ORG_ACCOUNTSYSTEM_L.FLocaleId, T_ORG_ACCOUNTSYSTEM_L.FNAME, T_ORG_ACCOUNTSYSTEM_L.FDESCRIPTION, T_ORG_ACCOUNTSYSTEM_L.FACCTSYSTEMID FROM T_ORG_ACCOUNTSYSTEM_L INNER JOIN T_ORG_ACCOUNTSYSTEM ON T_ORG_ACCOUNTSYSTEM_L.FACCTSYSTEMID = T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID WHERE T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID = @PKValue;
SELECT T_ORG_ACCTSYSENTRY.FEntryID, T_ORG_ACCTSYSENTRY.FMAINORGID, T_ORG_ACCTSYSENTRY.FACOUNTTYPE, T_ORG_ACCTSYSENTRY.FACCTCLDID, T_ORG_ACCTSYSENTRY.FDEFACCTPOLICY, T_ORG_ACCTSYSENTRY.FACCTPOLICYLST, T_ORG_ACCTSYSENTRY.FACCTSYSTEMID FROM T_ORG_ACCTSYSENTRY INNER JOIN T_ORG_ACCOUNTSYSTEM ON T_ORG_ACCTSYSENTRY.FACCTSYSTEMID = T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID WHERE T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID = @PKValue;
SELECT T_ORG_ACCTSYSENTRY_L.FPkId, T_ORG_ACCTSYSENTRY_L.FLocaleId, T_ORG_ACCTSYSENTRY_L.FACCTDESCRIPTION, T_ORG_ACCTSYSENTRY_L.FEntryID FROM T_ORG_ACCTSYSENTRY_L INNER JOIN T_ORG_ACCTSYSENTRY ON T_ORG_ACCTSYSENTRY_L.FEntryID = T_ORG_ACCTSYSENTRY.FEntryID INNER JOIN T_ORG_ACCOUNTSYSTEM ON T_ORG_ACCTSYSENTRY.FACCTSYSTEMID = T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID WHERE T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID = @PKValue;
SELECT T_ORG_MULTIAPPACCTPOLICY.FPkId, T_ORG_MULTIAPPACCTPOLICY.FMULBASE, T_ORG_MULTIAPPACCTPOLICY.FEntryID FROM T_ORG_MULTIAPPACCTPOLICY INNER JOIN T_ORG_ACCTSYSENTRY ON T_ORG_MULTIAPPACCTPOLICY.FEntryID = T_ORG_ACCTSYSENTRY.FEntryID INNER JOIN T_ORG_ACCOUNTSYSTEM ON T_ORG_ACCTSYSENTRY.FACCTSYSTEMID = T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID WHERE T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID = @PKValue;
SELECT T_ORG_ACCTSYSDETAIL.FDetailID, T_ORG_ACCTSYSDETAIL.FSUBORGID, T_ORG_ACCTSYSDETAIL.FINVESTRATE, T_ORG_ACCTSYSDETAIL.FEntryID FROM T_ORG_ACCTSYSDETAIL INNER JOIN T_ORG_ACCTSYSENTRY ON T_ORG_ACCTSYSDETAIL.FEntryID = T_ORG_ACCTSYSENTRY.FEntryID INNER JOIN T_ORG_ACCOUNTSYSTEM ON T_ORG_ACCTSYSENTRY.FACCTSYSTEMID = T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID WHERE T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID = @PKValue;
SELECT T_ORG_ACCTSYSDETAIL_L.FPkId, T_ORG_ACCTSYSDETAIL_L.FLocaleId, T_ORG_ACCTSYSDETAIL_L.FSUBDESCRIPTION, T_ORG_ACCTSYSDETAIL_L.FDetailID FROM T_ORG_ACCTSYSDETAIL_L INNER JOIN T_ORG_ACCTSYSDETAIL ON T_ORG_ACCTSYSDETAIL_L.FDetailID = T_ORG_ACCTSYSDETAIL.FDetailID INNER JOIN T_ORG_ACCTSYSENTRY ON T_ORG_ACCTSYSDETAIL.FEntryID = T_ORG_ACCTSYSENTRY.FEntryID INNER JOIN T_ORG_ACCOUNTSYSTEM ON T_ORG_ACCTSYSENTRY.FACCTSYSTEMID = T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID WHERE T_ORG_ACCOUNTSYSTEM.FACCTSYSTEMID = @PKValue;',N'@PKValue bigint',@PKValue=1

SELECT * FROM T_ORG_ACCOUNTSYSTEM_L

SELECT * FROM T_ORG_ACCTSYSENTRY 

SELECT * FROM  T_ORG_ACCTSYSENTRY_l
SELECT * from T_ORG_ACCTSYSDETAIL where FsubOrgid in(289823,289824)

