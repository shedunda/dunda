exec sp_executesql 
SELECT V.FDATE, VL.FNAME, V.FVOUCHERGROUPID, V.FVOUCHERGROUPNO, A.FACCTID, 
B.FACCTID foppacctid, VE.FDC, VE1.FDC foppdc, T.FAMOUNT famount, ISNULL(CF.FAMOUNT, 0) findirectamount,
 ISNULL(CF1.FAMOUNT, 0) foppindirectamount, C.FID, C1.FID foppid, T.FVOUCHERID, T.FVCHENTRYID, 
 T.FVCHOPPOENTRYID foppvchentryid 

FROM T_GL_SPLITVOUCHER T 
     LEFT OUTER JOIN T_GL_CASHFLOW C ON C.FID = T.FSUBITEMID
     LEFT OUTER JOIN T_GL_CASHFLOW C1 ON C1.FID = T.FOPPSUBITEMID 
     INNER JOIN T_GL_VOUCHER V ON V.FVOUCHERID = T.FVOUCHERID 
     INNER JOIN T_GL_VOUCHERENTRY VE ON VE.FENTRYID = T.FVCHENTRYID 
     INNER JOIN T_GL_VOUCHERENTRY VE1 ON VE1.FENTRYID = T.FVCHOPPOENTRYID 
     LEFT OUTER JOIN T_BD_VOUCHERGROUP_L VL ON (V.FVOUCHERGROUPID = VL.FVCHGROUPID AND FLOCALEID = @FLOCALEID) 
     LEFT OUTER JOIN T_GL_CASHFLOWBAL CF ON (T.FVCHENTRYID = CF.FVCHENTRYID AND T.FVCHOPPOENTRYID = CF.FVCHOPPOENTRYID) 
     LEFT OUTER JOIN T_GL_CASHFLOWBAL CF1 ON (T.FVCHOPPOENTRYID = CF1.FVCHENTRYID AND T.FVCHENTRYID = CF1.FVCHOPPOENTRYID) 
     INNER JOIN TMP0230AEB7A26F11E680BC1418776 A ON A.FACCTID = T.FACCOUNTID 
     INNER JOIN TMP0230AEB7A26F11E680BC1418776 B ON B.FACCTID = T.FOPPACCOUNTID 
     WHERE (
             (
               (
                 (
                   (
                     (
                       (
                         (
                           (
                             (T.FACCTBOOKID = @FACCTBOOKID AND A.FCASHTYPE = 0)
                             AND B.FCASHTYPE = 0
                            )
                             AND (T.FAMOUNTFOR <> 0 )
                           )
                             AND (T.FOPPAMOUNTFOR <> 0))
                             AND V.FINVALID = 0)
                              AND (V.FSOURCEBILLKEY <> ''33742a8f-813c-4ca9-989c-650289fad2d1''))
                               AND (((V.FYEAR * 100) + V.FPERIOD) BETWEEN 201610 AND 201610)) 
                               AND (A.FNUMBER IN (''1403'') OR B.FNUMBER IN (''1403''))) 
                               AND T.FCURRENCYID = 1) AND T.FOPPCURRENCYID = 1) 
                               ORDER BY FDATE ASC, FVOUCHERGROUPID ASC, FVOUCHERGROUPNO ASC',N'@FLOCALEID bigint,@FACCTBOOKID bigint',@FLOCALEID=2052,@FACCTBOOKID=289864
