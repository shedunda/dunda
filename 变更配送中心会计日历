SELECT * FROM (SELECT t0.FNUMBER fnumber, t0_L.FNAME fname, t0.FCURRENCYID fcurrencyid_id, t0.FACCTCALENDARID facctcalendarid_id, t0.FACCTGROUPID facctgroupid_id, t0.FRATETYPEID fratetypeid_id, t0.FDOCUMENTSTATUS fdocumentstatus, t0.FFORBIDSTATUS fforbidstatus, t0.FISSYSPRESET fissyspreset, t0.FISCALBYEXPITEM fiscalbyexpitem, t0.FISTAXAMOUNT fistaxamount, t0.FUSEORGID fuseorgid, t0.FUSEORGID fuseorgid_id, t0.FACCTPOLICYID facctpolicyid, t0.FMASTERID fmasterid, ROW_NUMBER() OVER( ORDER BY t0.FNUMBER ASC) fidentityid FROM T_FA_ACCTPOLICY t0 LEFT OUTER JOIN T_FA_ACCTPOLICY_L t0_L ON (t0.FACCTPOLICYID = t0_L.FACCTPOLICYID AND t0_L.FLocaleId = 2052) WHERE t0.FFORBIDSTATUS = 'A') tlist WHERE ((fidentityid >= 1) AND (fidentityid <= 200)) OPTION ( MAXDOP 0)

select * from  T_FA_ACCTPOLICY

--update T_FA_ACCTPOLICY set FACCTCALENDARID=1


select * from T_BD_ACCOUNTPERIOD where FID=100105

select * from T_BD_ACCOUNTBOOK where FNUMBER='G005' 

select * from T_BD_ACCOUNTCALENDAR  
